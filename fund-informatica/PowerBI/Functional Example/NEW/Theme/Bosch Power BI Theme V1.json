{
    "name": "BOSCH Data Visualization",
    "dataColors": ["#008ECF","#7FC6E7","#78BE20","#BBDE8F","#00A8B0","#7FD3D7","#50247F","#A791BF","#B90276","#DC80BA","#BFC0C2","#DFDFE0","#005691","#7FAAC8"],
    "background":"#FFFFFF",
    "foreground": "#000000",
	
	"visualStyles":{
		"*":{
			"*":{
				"*": [{
					"fontFamily": "Arial"
				}],
				"general":[{
					"responsive":true
				}],
				"labels": [{
					"show":true,
					"labelOrientation":"horizontal",
					"labelPosition":"Auto"
				}],
				"categoryAxis":[{
					"show":true,
					"labelColor":{ "solid": { "color": "#000000" } }
				}],
				"valueAxis":[{
					"show":true,
					"labelColor":{ "solid": { "color": "#000000" } }
				}],
				"legend":[{
					"show":true,
					"position":"Top"
				}]
				
			}
		},
	
		"line":{
				"lineStyles":[{
					"strokeWidth": 3,
					"showMarker": true,
					"markerShape": "square"
				}]
		},
		
		"tableEx":{
			"*":{
				"grid":[{
					"gridHorizontal":false,
					"gridVertical":false,
					"outlineColor":{ "solid": { "color": "#BFC0C2" } },
					"outlineWeight":2
				}],
				"columnHeaders":[{
					"outline":"BottomOnly"
				}],
				"values":[{
					"backColorSecondary":{ "solid": { "color": "#FFFFFF" } },
					"backColorPrimary":{ "solid": { "color": "#FFFFFF" } }
				}]
			}
		},
		
		"pivotTable":{
			"*":{
				"grid":[{
					"gridHorizontal":false,
					"gridVertical":false,
					"outlineColor":{ "solid": { "color": "#BFC0C2" } },
					"outlineWeight":2
				}],
				"columnHeaders":[{
					"outline":"BottomOnly"
				}],
				"rowHeaders":[{
					"outline":"RightOnly"
				}],
				"values":[{
					"backColorSecondary":{ "solid": { "color": "#FFFFFF" } },
					"backColorPrimary":{ "solid": { "color": "#FFFFFF" } }
				}]
			}
		},
		
		"treemap":{
			"*":{
				"legend":[{
					"show":false
				}]
			}
		}
	}
}